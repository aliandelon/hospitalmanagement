Options +FollowSymlinks
RewriteEngine on

# deal with admin first
RewriteCond %{REQUEST_URI} ^/hospitalmanagement/(admin) 
RewriteRule ^admin/assets/(.*)$ backend/web/assets/$1 [L]
RewriteRule ^admin/css/(.*)$ backend/web/css/$1 [L]
RewriteRule ^admin/plugins/(.*)$ backend/web/plugins/$1 [L]
RewriteRule ^admin/js/(.*)$ backend/web/js/$1 [L]
RewriteRule ^admin/img/(.*)$ backend/web/img/$1 [L]
RewriteRule ^admin/fonts/(.*)$ backend/web/fonts/$1 [L]

RewriteCond %{REQUEST_URI} !^/hospitalmanagement/backend/web/(assets|css|plugins|js|fonts|img)/ 
RewriteCond %{REQUEST_URI} ^/hospitalmanagement/(admin)
RewriteRule ^.*$ backend/web/index.php [L]

RewriteCond %{REQUEST_URI} ^/hospitalmanagement/(uploads)
RewriteRule ^(uploads)($|/) - [L]


RewriteCond %{REQUEST_URI} ^/hospitalmanagement/(assets|css|js|images|video) 
RewriteRule ^assets/(.*)$ frontend/web/assets/$1 [L]
RewriteRule ^css/(.*)$ frontend/web/css/$1 [L]
RewriteRule ^js/(.*)$ frontend/web/js/$1 [L]
RewriteRule ^images/(.*)$ frontend/web/images/$1 [L]
RewriteRule ^video/(.*)$ frontend/web/video/$1 [L]

RewriteCond %{REQUEST_URI} !^/hospitalmanagement/(frontend|backend)/web/(assets|css)/ 
RewriteCond %{REQUEST_URI} !index.php
RewriteCond %{REQUEST_FILENAME} !-f [OR]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^.*$ frontend/web/index.php